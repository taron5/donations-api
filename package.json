{
  "name": "donations",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --inspect ./bin/www",
    "start": "node ./bin/www",
    "debug": "node --inspect-brk app.js",
    "watch": "nodemon --inspect ./bin/www",
    "dasha-start": "ts-node -r dotenv/config ./RangeTalkAIDasha/main.ts",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:seed": "npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo --name name-of-your-migration",
    "db:migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "db:seed": "npx sequelize-cli db:seed --seed name_of_your_seed.js",
    "/": "warning js extension is required",
    "db:seed:all": "npx sequelize-cli db:seed:all",
    "db:seed:undo:all": "npx sequelize-cli db:seed:undo:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo --seed name_of_your_seed.js",
    "//": "warning js extension is required",
    "db:fresh": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "model:generate": "npx sequelize-cli model:generate --name name_of_your_model --attributes fiield:type",
    "migrate:generate": "sequelize migration:create --name name_of_your_migration",
    "git:ignore": "git rm --cached folder/name_of_file",
    "lint:prettier": "eslint **/*.js",
    "lint:prettier:fix": "eslint **/*.js --fix"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "joi": "^17.9.1",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1",
    "mysql2": "^3.2.1",
    "sequelize": "^6.31.0"
  }
}
